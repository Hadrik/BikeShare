@{
    ViewData["Title"] = "Home Page";
}

@if (User.Identity!.IsAuthenticated)
{
@section Styles {
    <link rel="stylesheet" href="~/css/home.css" />
    <link rel="stylesheet" href="~/lib/leaflet/leaflet.css" />
}
@section Scripts {
    <script src="~/lib/leaflet/leaflet.js" type="text/javascript"></script>
    <script src="~/js/home.js" type="text/javascript" defer></script>
}
    <div id="map"></div>
}
else
{
@section Styles {
    <link rel="stylesheet" href="~/css/home.css" />
}
    <div class="w-container">
        <h1>Bike Bike Bike</h1>
        <form>
            <button type="submit" asp-controller="Auth" asp-action="Register">Start driving</button>
        </form>
    </div>
}
